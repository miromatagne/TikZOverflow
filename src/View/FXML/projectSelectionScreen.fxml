<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.ViewControllers.ProjectSelectionViewController">
   <children>
      <GridPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #4D4D4D;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="1.7976931348623157E308" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="103.0" minHeight="10.0" percentHeight="30.0" prefHeight="103.0" valignment="CENTER" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="364.0" minHeight="10.0" percentHeight="70.0" valignment="CENTER" vgrow="SOMETIMES" />
            <RowConstraints />
        </rowConstraints>
         <children>
            <GridPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" style="-fx-background-color: #4D4D4D;">
                <columnConstraints>
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                    <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
                </columnConstraints>
                <rowConstraints>
                    <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES" />
                    <RowConstraints maxHeight="1.7976931348623157E308" vgrow="SOMETIMES" />
                </rowConstraints>
                <Label alignment="CENTER" text="TikZOverflow" textFill="WHITE" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
                    <font>
                        <Font size="29.0" />
                    </font>
                </Label>
                <HBox prefHeight="200.0" prefWidth="200.0">
                    <Button mnemonicParsing="false" onAction="#modificationButtonAction" style="-fx-background-color: white; -fx-border-insets: 0; -fx-focus-traversable: false;" text="Modification">
                        <font>
                            <Font name="System Bold" size="12.0" />
                        </font>
                        <HBox.margin>
                            <Insets right="5.0" />
                        </HBox.margin>
                    </Button>
                    <Button alignment="CENTER" mnemonicParsing="false" onAction="#logout" style="-fx-background-color: white; -fx-border-insets: 0; -fx-focus-traversable: false;" text="Log Out" GridPane.halignment="RIGHT" GridPane.valignment="TOP">
                        <font>
                            <Font name="System Bold" size="12.0" />
                        </font>
                    </Button>
                </HBox>
                <HBox prefHeight="100.0" prefWidth="200.0" GridPane.rowIndex="1">
                    <padding>
                        <Insets bottom="10.0" left="10.0" right="10.0" top="5.0" />
                    </padding>
                </HBox>
                <HBox alignment="BOTTOM_RIGHT" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2" GridPane.rowIndex="1">
                    <children>
                        <Button fx:id="copyButton" mnemonicParsing="false" onAction="#onPressCopy" text="Copy">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="deleteButton" mnemonicParsing="false" onAction="#onPressDelete" text="Delete">
                            <HBox.margin>
                                <Insets left="5.0" right="5.0" />
                            </HBox.margin>
                        </Button>
                        <Button fx:id="createButton" mnemonicParsing="false" onAction="#onCreateButton" text="Create">
                            <HBox.margin>
                                <Insets left="5.0" />
                            </HBox.margin>
                        </Button>
                    </children>
                  <GridPane.margin>
                     <Insets />
                  </GridPane.margin>
                </HBox>
                  </GridPane>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="200.0" style="-fx-background-color: #4D4D4D; -fx-font-size: 16; -fx-text-fill: white;" GridPane.rowIndex="1">
              <columns>
                <TableColumn fx:id="checkBoxColumn" editable="false" prefWidth="${tableView.width*0.05}" resizable="false" sortable="false" style="-fx-background-color: #404040;" />
                <TableColumn fx:id="titleColumn" editable="false" resizable="false" sortable="false" style="-fx-background-color: #404040; -fx-text-fill: white;" text="Title" />
                  <TableColumn fx:id="ownerColumn" editable="false" prefWidth="${tableView.width*0.25}" resizable="false" sortable="false" style="-fx-background-color: #404040;" text="Owner" />
                  <TableColumn fx:id="dateColumn" editable="false" prefWidth="${tableView.width*0.25}" resizable="false" sortable="false" style="-fx-background-color: #404040;" text="Modification Date" />
                  <TableColumn fx:id="renameColumn" editable="false" prefWidth="${tableView.width*0.10}" resizable="false" sortable="false" style="-fx-background-color: #404040;" text="Rename" />
                  <TableColumn fx:id="shareColumn" editable="false" prefWidth="${tableView.width*0.10}" resizable="false" sortable="false" style="-fx-background-color: #404040; -fx-border-color: transparent;" text="Share" />
              </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </GridPane>
   </children>
</AnchorPane>
